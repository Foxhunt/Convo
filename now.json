{
    "name": "convos.art",
    "alias": [
      "convos.art"
    ],
    "version": 2,
    "regions": ["all"],
    "builds": [
        { "src": "www/package.json", "use": "@now/next" }
    ],
    "routes": [
        {
          "src": "/room/(.*)",
          "dest": "/www/room?roomId=$1",
          "headers": {
            "cache-control": "max-age=3600, s-maxage=432000"
          }
        },
        {
          "src": "/(.*)",
          "dest": "/www/$1",
          "headers": {
            "x-request-path": "$1",
            "cache-control": "max-age=3600, s-maxage=432000"
          }
        }
    ]
  } 
  